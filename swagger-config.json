            }
          }
        },
        "responses": {
          "200": {
            "description": "Sign out success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "default": "200"
                    },
                    "status": {
                      "type": "string",
                      "default": "OK"
                    },
                    "data":{
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "default": "Sign out success"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "No Refresh Token Provided",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "default": "401"
                    },
                    "status": {
                      "type": "string",
                      "default": "Unauthorized"
                    },
                    "errors":{
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "default": "No refresh token provided"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/user/token" : {
      "post": {
        "tags": ["User"],
        "summary": "Get new access token",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sign out success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "default": "200"
                    },
                    "status": {
                      "type": "string",
                      "default": "OK"
                    },
                    "data":{
                      "type": "object",
                      "properties": {
                        "accessToken": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "No refresh token, refresh token invalid or token expired",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "default": "401"
                    },
                    "status": {
                      "type": "string",
                      "default": "Unauthorized"
                    },
                    "errors":{
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "default": "Error message"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/user/{username}" : {
      "get" : {
        "tags": ["User"],
        "summary": "Get user details",
        "responses": {

        },
        "parameters": [
          {
            "name": "username",
