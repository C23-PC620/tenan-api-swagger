              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "default": "200"
                    },
                    "status": {
                      "type": "string",
                      "default": "OK"
                    },
                    "data":{
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "default": "Success"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "id_attraction",
            "in": "path",
            "required": true,
            "description": "attraction id",
            "schema" : {
              "type": "string"
            }
          }
        ]
      }
    }
  }
}